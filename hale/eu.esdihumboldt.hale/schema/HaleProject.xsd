		</xs:sequence>
	</xs:complexType>
	<xs:element name="ContextElement" type="xs:string"/>
	<xs:element name="Styles" type="Styles"/>
	<xs:complexType name="Styles">
		<xs:sequence>
			<xs:element name="path" type="xs:string" minOccurs="1" maxOccurs="1"/>
