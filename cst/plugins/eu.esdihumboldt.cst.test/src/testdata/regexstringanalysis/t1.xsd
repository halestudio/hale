    	<sequence>
    		<element maxOccurs="1" minOccurs="1" name="a1" type="string"/>
    		<element maxOccurs="1" minOccurs="1" name="b1" type="string"/>
    		<element maxOccurs="1" minOccurs="1" name="c1" type="string"/>
