    <cell relation="eu.esdihumboldt.cst.functions.groovy.join" id="C3b5c23dd-e22a-4462-bcbd-0b6092c85b4c" priority="normal">
        <source name="types">
            <class>
                <type name="Connection" ns="A"/>
            </class>
        </source>
        <source name="types">
            <class>
                <type name="Lake" ns="A"/>
            </class>
        </source>
        <target>
            <class>
                <type name="LakeFlow" ns="B"/>
            </class>
        </target>
        <complexParameter name="join">
            <jp:join-parameter xmlns:jp="http://www.esdi-humboldt.eu/hale/join">
                <class>
                    <type name="Lake" ns="A"/>
                </class>
                <class>
                    <type name="Connection" ns="A"/>
                </class>
                <jp:condition>
                    <property>
                        <type name="Lake" ns="A"/>
                        <child name="id" ns="A"/>
                    </property>
                    <property>
                        <type name="Connection" ns="A"/>
                        <child name="standingId" ns="A"/>
                    </property>
                </jp:condition>
            </jp:join-parameter>
        </complexParameter>
        <complexParameter name="script">
            <core:text xmlns:core="http://www.esdi-humboldt.eu/hale/core" xml:space="preserve">
_target {
	// create inflow/outflow properties from associated connections
	_source.links.Connection.each {	connection -&gt;
		if (connection.p.direction.value() == 'in') {
		  inflow {
		  	riverId( connection.p.streamId.value() )
		  }
		}
		else {
		  outflow {
		  	riverId( connection.p.streamId.value() )
		  }
		}
	}
}
</core:text>
        </complexParameter>
    </cell>
