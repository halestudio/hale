<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="eu.esdihumboldt.hale.schema.valueconstraint" name="Constraints represented as Values" schema="schema/eu.esdihumboldt.hale.schema.valueconstraint.exsd"/>
   <extension
         point="eu.esdihumboldt.hale.io.action">
      <action
            categoryName="Source schemas"
            id="eu.esdihumboldt.hale.io.schema.read.source"
            name="Load source schema"
            resourceName="source schema"
            type="eu.esdihumboldt.hale.common.schema.io.SchemaReader">
      </action>
      <action
            categoryName="Target schemas"
            id="eu.esdihumboldt.hale.io.schema.read.target"
            name="Load target schema"
            resourceName="target schema"
            type="eu.esdihumboldt.hale.common.schema.io.SchemaReader">
      </action>
      <action
            id="eu.esdihumboldt.hale.io.schema.export.target"
            name="Export target schemas"
            type="eu.esdihumboldt.hale.common.schema.io.SchemaWriter">
      </action>
      <action
            id="eu.esdihumboldt.hale.io.schema.export.source"
            name="Export source schemas"
            type="eu.esdihumboldt.hale.common.schema.io.SchemaWriter">
      </action>
   </extension>
   <extension
         point="eu.esdihumboldt.hale.schema.valueconstraint">
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.property.factory.CardinalityFactory"
            id="cardinality"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.property.Cardinality">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.property.factory.ChoiceFlagFactory"
            id="choice"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.property.ChoiceFlag">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.property.factory.NillableFlagFactory"
            id="nillable"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.property.NillableFlag">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.property.factory.UniqueConstraintFactory"
            id="unique-context"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.property.Unique">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.type.factory.AbstractFlagFactory"
            id="abstract"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.type.AbstractFlag">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.type.factory.AugmentedValueFlagFactory"
            id="augmented-value"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.type.AugmentedValueFlag">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.type.factory.HasValueFlagFactory"
            id="has-value"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.type.HasValueFlag">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.type.factory.MappableFlagFactory"
            id="mappable"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.type.MappableFlag">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.type.factory.MappingRelevantFlagFactory"
            id="mapping-relevant"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.type.MappingRelevantFlag">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.type.factory.BindingFactory"
            id="binding"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.type.Binding">
      </valueconstraint>
      <valueconstraint
            factory="eu.esdihumboldt.hale.common.schema.model.constraint.type.factory.EnumerationFactory"
            id="enumeration"
            type="eu.esdihumboldt.hale.common.schema.model.constraint.type.Enumeration">
      </valueconstraint>
   </extension>

</plugin>
