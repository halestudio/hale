                    <sourceExpression>
                        <OCQL>'lcv'</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:name</targetAttribute>
                    <sourceExpression>
                        <OCQL>'LCRT_07_13'</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:nomenclatureDocumentation</targetAttribute>
                    <sourceExpression>
                        <OCQL>'bla bla bla'</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:extent/gmd:EX_Extent/gmd:geographicElement/gmd:EX_BoundingPolygon/gmd:polygon/gml:AbstractGeometry</targetAttribute>
