                        <OCQL>geom</OCQL>
                    </sourceExpression>
                    <targetAttributeNode>gml:MultiSurfaceType</targetAttributeNode>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
        <FeatureTypeMapping>
            <sourceDataStore>dataStore</sourceDataStore>
            <sourceType>landcover_norm</sourceType>
            <targetElement>lcv:LandCoverUnit</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>FEATURE_LINK</targetAttribute>
                    <sourceExpression>
                        <OCQL>dataset_id</OCQL>
                    </sourceExpression>
