                <AttributeMapping>
                    <targetAttribute>lcv:landCoverObservation[2]/lcv:LandCoverObservation/lcv:observationDate</targetAttribute>
                    <sourceExpression>
                        <OCQL>if_then_else(isNull(ucs2013), Expression.NIL, '2013-01-01 00:00:00.0')</OCQL>
                    </sourceExpression>
