                    </idExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:inspireId/base:Identifier/base:localId</targetAttribute>
                    <sourceExpression>
                        <OCQL>uuid_v1</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:inspireId/base:Identifier/base:namespace</targetAttribute>
                    <sourceExpression>
                        <OCQL>'lcu'</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:landCoverObservation[1]/lcv:LandCoverObservation/lcv:class</targetAttribute>
                    <sourceExpression>
                        <OCQL>ucs2007</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:landCoverObservation[1]/lcv:LandCoverObservation/lcv:observationDate</targetAttribute>
                    <sourceExpression>
                        <OCQL>if_then_else(isNull(ucs2007), Expression.NIL, '2007-01-01 00:00:00.0')</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>lcv:landCoverObservation[2]/lcv:LandCoverObservation/lcv:class</targetAttribute>
                    <sourceExpression>
                        <OCQL>ucs2013</OCQL>
